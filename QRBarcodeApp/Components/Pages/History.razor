@page "/history"

@inherits CodeListBase

@inject LocalStorageService LocalStorageService

@{
    base.BuildRenderTree(__builder);
}

@code {
    protected override async Task LoadComponentDataAsync()
    {
        await LocalStorageService.SaveActiveTabAsync("/history");
        codeList = await LocalStorageService.GetCodesAsync();
        tabName = "History";
    }
}
